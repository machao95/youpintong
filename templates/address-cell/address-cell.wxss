.address-box {
  overflow: hidden;
  background: #ffffff;
  margin-bottom: 20rpx;
}
.address-image {
  height: 6px;
  background-image: url(data:image/png;base64,);
}
.address-car {
  width: 40rpx;
  height: 36rpx;
  padding-right: 20rpx;
}
.address-content {
  padding: 40rpx 20rpx;
  color: #333;
}
.address-car image {
  width: 100%;
  height: 100%;
}
.address-center {
  flex: 1;
  line-height: 50rpx;
}
.address-title {
  font-weight: bold;
  font-size: var(--fontSize-lg);
}
.address-detail {
  font-size: var(--fontSize-normal);
}
.address-action {
  padding-left: 20rpx;
}
