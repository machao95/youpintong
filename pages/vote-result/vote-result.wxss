
.page-result {
  padding: 80rpx 40rpx 40rpx 20rpx;
}

.result-item {
  overflow: hidden;
  margin-bottom: 40rpx;
}
.label {
  width: 160rpx;
  text-align: right;
  padding-right: 20rpx;
}
.rate-box {
  flex: 1;
  border: 1px solid #f2f2f2;
  height: 30rpx;
  border-radius: 30rpx;
}
.rate {
  background: var(--color-info);
  height: 100%;
  border-radius: 30rpx;
  width: 60%;
}
.value-box {
  padding-left: 20rpx;
}
